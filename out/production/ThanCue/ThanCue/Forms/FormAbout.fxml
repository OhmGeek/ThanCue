<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Hyperlink?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ThanCue.Forms.FormAboutController"
            fx:id="anchor_pane">
    <children>
        <VBox fx:id="vbox" alignment="TOP_CENTER" spacing="8">
            <Label fx:id="lblAboutInfo" text=""/>
            <Hyperlink fx:id="btnThanCue" text="ThanCue on GitHub @ "/>
            <Hyperlink fx:id="btnMike" text="Mike on GitHub @ "/>
            <Hyperlink fx:id="btnRyan" text="Ryan on GitHub @ "/>
            <Button fx:id="btnClose" text="Back"/>
        </VBox>
    </children>
</AnchorPane>